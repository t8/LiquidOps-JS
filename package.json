{
  "name": "liquidops",
  "version": "0.1.0",
  "author": "Lorimer Jenkins (lorimer@liquidops.io), Marton Lederer (marton@liquidops.io)",
  "license": "MIT",
  "description": "LiquidOps is an over-collateralised lending and borrowing protocol built on Arweave's L2 AO.",
  "homepage": "https://liquidops.io",
  "bugs": {
    "url": "https://github.com/Othent/KMS-JS-new/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "keywords": [
    "Arweave",
    "LiquidOps",
    "Community Labs",
    "AO",
    "Defi",
    "AR",
    "Lending",
    "Borrowing"
  ],
  "scripts": {
    "dev": "bun run build && bun run link",
    "build": "bun run prettier && tsup src/index.ts --format cjs,esm --dts",
    "test": "bun test",
    "prettier": "prettier --write .",
    "link": "bun link",
    "patch:aoconnect": "bunx patch-package @permaweb/aoconnect",
    "patch:aoscheduler": "bunx patch-package @permaweb/ao-scheduler-utils",
    "postinstall": "patch-package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/useLiquidOps/package.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/",
    "src/",
    "LICENSE",
    "package.json",
    "README.md"
  ],
  "dependencies": {
    "@permaweb/ao-scheduler-utils": "^0.0.24",
    "@permaweb/aoconnect": "^0.0.59",
    "ao-tokens": "^0.0.4",
    "ar-gql": "^2.0.2",
    "arbundles": "^0.11.2",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "prettier": "^3.0.3",
    "ts-jest": "^29.1.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.4",
    "bun-types": "^1.1.27"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  }
}
